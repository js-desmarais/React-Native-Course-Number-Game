{
	"expo": {
		"name": "number-game",
		"slug": "number-game",
		"version": "1.0.0",
		"orientation": "default",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-font"]
	}
}
